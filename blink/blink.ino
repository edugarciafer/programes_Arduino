//********** Setup ****************************************************************
void setup(){
// inicia el pin com a sortida



pinMode(13, OUTPUT);
}

//********** Loop *****************************************************************
void loop()
{
digitalWrite(13, HIGH) ; // engega el led

delay( 1000) ; // espera un segon

digitalWrite(13, LOW) ; // atura el led

delay( 1000) ; // espera un segon
}

